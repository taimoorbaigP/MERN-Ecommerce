  "bugs": {
    "url": "https://github.com/taimoorbaigP/MERN-Ecommerce/issues"
  },
  "homepage": "https://github.com/taimoorbaigP/MERN-Ecommerce#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.4.6"
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19"
  }
}
